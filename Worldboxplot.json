{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {"url": "https://raw.githubusercontent.com/Monash64/FIT3179/main/Australian_Exports.csv",
  "format": {"type": "csv"}},
  "width": 800,
  "height": 300,
  "mark": {"type": "boxplot"},
  "encoding": {
    "x": {
      "field": "Year",
      "type":"temporal",
      "title": "Year"
    },
    "y": {
      "field": "Export_Total",
      "type":"quantitative",   
      "title": "Volume exported ('000 kt)"
    },
    "color": {"value" : "#008000"},
    "tooltip": [
      {"field": "Export_Total", "type": "quantitative", "title": "Wheat (kt)"},
      {"field": "Country", "type": "nominal", "title": "Country"},
      {"field": "Year", "timeUnit": "year", "type": "temporal", "title": "Year"},
      {"field": "Year", "timeUnit": "year", "type": "temporal", "title": "Year"}]
  },
  "config": {}
}
